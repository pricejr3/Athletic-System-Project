create sequence new_athlete_id 			increment 1 start 50000;
create sequence new_coaches_report_id		increment 1 start 50000;
create sequence new_family_history_id 		increment 1 start 50000;
create sequence new_health_appraisal_id 	increment 1 start 50000;
create sequence new_health_history_id 		increment 1 start 50000;
create sequence new_injury_addtnl_info_id	increment 1 start 50000;
create sequence new_injury_report_id		increment 1 start 50000;
create sequence new_insurance_id 		increment 1 start 50000;
create sequence new_personal_addtnl_info_id	increment 1 start 50000;
create sequence new_physical_id 		increment 1 start 50000;
create sequence new_physician_remark_id 	increment 1 start 50000;
create sequence new_recommended_treatment_id	increment 1 start 50000;
create sequence new_treatment_addtnl_info_id 	increment 1 start 50000;
create sequence new_treatment_record_id 	increment 1 start 30000;
create sequence new_treatment_remark_id 	increment 1 start 50000;
create sequence new_waiver_id 			increment 1 start 50000;
create sequence new_athlete_release_id 		increment 1 start 1;
create sequence new_ferpa_release_id 		increment 1 start 1;
create sequence new_health_info_release_id 	increment 1 start 1;
create sequence new_health_history2_id 		increment 1 start 1;

grant select, update on new_athlete_id			to musmdb_user;
grant select, update on new_coaches_report_id		to musmdb_user;
grant select, update on new_family_history_id           to musmdb_user;
grant select, update on new_health_appraisal_id         to musmdb_user;
grant select, update on new_health_history_id           to musmdb_user;
grant select, update on new_injury_addtnl_info_id       to musmdb_user;
grant select, update on new_injury_report_id            to musmdb_user;
grant select, update on new_insurance_id                to musmdb_user;
grant select, update on new_personal_addtnl_info_id     to musmdb_user;
grant select, update on new_physical_id                 to musmdb_user;
grant select, update on new_physician_remark_id         to musmdb_user;
grant select, update on new_recommended_treatment_id    to musmdb_user;
grant select, update on new_treatment_addtnl_info_id    to musmdb_user;
grant select, update on new_treatment_record_id         to musmdb_user;
grant select, update on new_treatment_remark_id         to musmdb_user;
grant select, update on new_waiver_id                   to musmdb_user;
grant select, update on new_athlete_release_id          to musmdb_user;
grant select, update on new_ferpa_release_id            to musmdb_user;
grant select, update on new_health_info_release_id      to musmdb_user;
grant select, update on new_health_history2_id          to musmdb_user;
