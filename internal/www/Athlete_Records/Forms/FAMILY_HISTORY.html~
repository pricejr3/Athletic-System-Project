--- BEGIN PREPOPULATED FIELD STATEMENTS ---
SELECT

LAST_NAME, FIRST_NAME, MIDDLE_NAME, SPORT,
TO_CHAR(now(), 'MM-DD-YYYY') as FORM_DATE

from athlete_lv

WHERE ORIG_VERSION_ID = $ORIG_ATHLETE_ID;
--- END PREPOPULATED FIELD STATEMENTS ---


